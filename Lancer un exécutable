#include <stdio.h>
#include <stdlib.h>

int main (void)

{
system("start Xtremsplit.exe"); //quand le fichier est dans le même répertoire
system("start ../Xtremsplit.exe"); //pour un executable d'un dossier antérieure
system("start C:\\WINDOWS\\explorer.exe") //chemin absolue

system("PAUSE");
return 0;
}

