#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <sys/wait.h>

int main(int argv, char *argc)
{
int pid;

pid = fork();

switch(pid)

        {
        case -1:
        printf("erreur");
        break;

        case 0:
        execl("/home/raii/Documents/histo","histo","histo.c",NULL);
        break;

        default:
        wait(NULL);
        break;
        }

return 0;
}

